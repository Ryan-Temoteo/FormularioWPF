    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:FormularioWPF"
    StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>

            <!--#region 1- Cores-->

            <LinearGradientBrush x:Key="FundoMainBgrn_Black" StartPoint="0.5,0" EndPoint="0.5,1">
                <GradientStop Offset="0" Color="#FF1E1E1E" />
                <GradientStop Offset="1" Color="#FF191919" />
            </LinearGradientBrush>

            <SolidColorBrush x:Key="FundoStroke_Black" Color="Black" />

            <LinearGradientBrush x:Key="FundoTileBgrnd_Black" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0" EndPoint="0.5,1">
                <GradientStop Offset="1" Color="#FF515151" />
                <GradientStop Color="#FF323131" />
            </LinearGradientBrush>

            <LinearGradientBrush x:Key="FundoMainBgrn_Blue" StartPoint="0.5,0" EndPoint="0.5,1">
                <GradientStop Offset="0" Color="#FF003164" />
                <GradientStop Offset="1" Color="#FF073768" />
            </LinearGradientBrush>

            <SolidColorBrush x:Key="FundoStroke_Blue" Color="#FF142841" />

            <LinearGradientBrush x:Key="FundoTileBgrnd_Blue" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0" EndPoint="0.5,1">
                <GradientStop Offset="0.833" Color="#FF0D559F" />
                <GradientStop Color="#FF064484" />
            </LinearGradientBrush>

            <LinearGradientBrush x:Key="FundoMainBgrn_Green" StartPoint="0.5,0" EndPoint="0.5,1">
                <GradientStop Offset="0" Color="#FF006436" />
                <GradientStop Offset="1" Color="#FF07683B" />
            </LinearGradientBrush>

            <SolidColorBrush x:Key="FundoStroke_Green" Color="#FF14411D" />

            <LinearGradientBrush x:Key="FundoTileBgrnd_Green" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0" EndPoint="0.5,1">
                <GradientStop Offset="1" Color="#FF0CAC5A" />
                <GradientStop Color="#FF068444" />
            </LinearGradientBrush>

            <LinearGradientBrush x:Key="FundoMainBgrn_Purple" StartPoint="0.5,0" EndPoint="0.5,1">
                <GradientStop Offset="0" Color="#FF500064" />
                <GradientStop Offset="1" Color="#FF590768" />
            </LinearGradientBrush>

            <SolidColorBrush x:Key="FundoStroke_Purple" Color="#FF3C1441" />

            <RadialGradientBrush x:Key="FundoTileBgrnd_Purple" GradientOrigin="0.582,0.409">
                <RadialGradientBrush.RelativeTransform>
                    <TransformGroup>
                        <ScaleTransform CenterX="0.5" CenterY="0.5" ScaleX="2.525" ScaleY="2.525" />
                        <SkewTransform AngleX="0" AngleY="0" CenterX="0.5" CenterY="0.5" />
                        <RotateTransform Angle="0" CenterX="0.5" CenterY="0.5" />
                        <TranslateTransform X="0.378" Y="-0.378" />
                    </TransformGroup>
                </RadialGradientBrush.RelativeTransform>
                <GradientStop Offset="0" Color="#FF6F1780" />
                <GradientStop Offset="1" Color="#FFA249B2" />
            </RadialGradientBrush>

            <LinearGradientBrush x:Key="FundoMainBgrn_Red" StartPoint="0.5,0" EndPoint="0.5,1">
                <GradientStop Offset="0" Color="#FF640000" />
                <GradientStop Offset="1" Color="#FF681007" />
            </LinearGradientBrush>

            <SolidColorBrush x:Key="FundoStroke_Red" Color="#FF411414" />

            <LinearGradientBrush x:Key="FundoTileBgrnd_Red" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0" EndPoint="0.5,1">
                <GradientStop Offset="1" Color="#FFA03F3F" />
                <GradientStop Color="#FF951D1D" />
            </LinearGradientBrush>

            <LinearGradientBrush x:Key="FundoMainBgrn_Rosa" StartPoint="0.5,0" EndPoint="0.5,1">
                <GradientStop Offset="0.494" Color="#FFF186F1" />
                <GradientStop Offset="1" Color="#FFF395F3" />
            </LinearGradientBrush>

            <SolidColorBrush x:Key="FundoStroke_Rosa" Color="DeepPink" />

            <RadialGradientBrush x:Key="FundoTileBgrnd_Rosa" GradientOrigin="0.582,0.409">
                <RadialGradientBrush.RelativeTransform>
                    <TransformGroup>
                        <ScaleTransform CenterX="0.5" CenterY="0.5" ScaleX="2.525" ScaleY="2.525" />
                        <SkewTransform AngleX="0" AngleY="0" CenterX="0.5" CenterY="0.5" />
                        <RotateTransform Angle="0" CenterX="0.5" CenterY="0.5" />
                        <TranslateTransform X="0.378" Y="-0.378" />
                    </TransformGroup>
                </RadialGradientBrush.RelativeTransform>
                <GradientStop Offset="0" Color="#FFF18EF1" />
                <GradientStop Offset="1" Color="#FFF095F0" />
            </RadialGradientBrush>

            <LinearGradientBrush x:Key="FundoMainBgrn_Vane" StartPoint="0.5,0" EndPoint="0.5,1">
                <GradientStop Color="#FF634DE4" />
                <GradientStop Offset="1" Color="#FF634DE4" />
            </LinearGradientBrush>

            <SolidColorBrush x:Key="FundoStroke_Vane" Color="#FF4631BF" />

            <LinearGradientBrush x:Key="FundoTileBgrnd_Vane" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0" EndPoint="0.5,1">
                <GradientStop Color="#FF6752E6" />
                <GradientStop Offset="1" Color="#FF8370F7" />
            </LinearGradientBrush>

            <LinearGradientBrush x:Key="FundoPadrao" StartPoint="0.5,0" EndPoint="0.5,1">
                <GradientStop Offset="0.833" Color="#FF0D559F" />
                <GradientStop Color="#FF064484" />
            </LinearGradientBrush>

            <Style x:Key="PropriedadesTela" TargetType="Window">
                <Setter Property="WindowState" Value="Normal" />
                <Setter Property="Background" Value="{DynamicResource FundoPadrao}" />
                <!--<EventSetter Event="KeyDown" Handler="EventTrigger_KeyDown" />
                <EventSetter Event="MouseMove" Handler="Window_MouseMove" />
                <EventSetter Event="Loaded" Handler="Window_Loaded" />-->
            </Style>

            <!--#endregion-->

            <!--#region 2- Botão-->

            <Style x:Key="BotaoBordaRedonda" TargetType="{x:Type Button}">
                <Setter Property="FontSize" Value="16" />
                <Setter Property="Foreground" Value="Black" />
                <Setter Property="FontFamily" Value="Roboto" />
                <Setter Property="FontWeight" Value="Medium" />

                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type Button}">
                            <Grid x:Name="grid">
                                <Border
                                    x:Name="border"
                                    CornerRadius="5"
                                    Cursor="Hand">
                                    <Border.Background>
                                        <SolidColorBrush Color="White" />
                                    </Border.Background>
                                    <ContentPresenter
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        TextElement.FontWeight="Medium" />
                                </Border>
                            </Grid>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter TargetName="border" Property="Background">
                                        <Setter.Value>
                                            <SolidColorBrush Color="#D2E3DB" />
                                        </Setter.Value>
                                    </Setter>
                                </Trigger>
                                <Trigger Property="IsPressed" Value="True">
                                    <Setter TargetName="border" Property="Background">
                                        <Setter.Value>
                                            <SolidColorBrush Color="#4B906D" />
                                        </Setter.Value>
                                    </Setter>
                                </Trigger>

                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>

